&mt {
    flavor = "balanced";
    retro-tap;
    require-prior-idle-ms = <120>;
};

&lt {
    flavor = "balanced";
    retro-tap;
    require-prior-idle-ms = <120>;
};

/ {
    combos {
        compatible = "zmk,combos";

        leftParet {
            bindings = <&kp LEFT_PARENTHESIS>;
            key-positions = <6 7>;
        };

        rightParet {
            bindings = <&kp RIGHT_PARENTHESIS>;
            key-positions = <7 8>;
        };

        minusCombo {
            bindings = <&kp MINUS>;
            key-positions = <32 33>;
        };

        capslock {
            bindings = <&caps_word>;
            key-positions = <4 5>;
        };

        printscreen {
            bindings = <&kp LG(LS(N5))>;
            key-positions = <1 2 3>;
            timeout-ms = <40>;
        };

        apost {
            bindings = <&kp APOSTROPHE>;
            key-positions = <14 15>;
        };

        c1 {
            bindings = <&kp NUMBER_1>;
            key-positions = <0 31>;
        };
    };

    behaviors {
        rht: repeatHoldTap {
            compatible = "zmk,behavior-hold-tap";
            label = "REPEATHOLDTAP";
            bindings = <&mo>, <&kp>;

            #binding-cells = <2>;
            flavor = "balanced";
            tapping-term-ms = <120>;
            quick-tap-ms = <200>;
            retro-tap;
            require-prior-idle-ms = <120>;
        };
    };

    macros {
        euro: euro {
            compatible = "zmk,behavior-macro";
            #binding-cells = <0>;
            bindings = <&kp LA(NUMBER_0) &kp LA(NUMBER_1) &kp LA(NUMBER_2) &kp LA(N8)>;
            label = "EURO";
        };
    };

    keymap {
        compatible = "zmk,keymap";

        qwerty {
            bindings = <
&lt 3 Q             &lt 4 W      &lt 5 E           &lt 6 R       &lt 7 T         &lt 8 Y         &lt 9 U       &lt 10 I      &lt 11 O             &lt 12 P
&mt LEFT_CONTROL A  &mt LCTRL S  &mt LEFT_ALT D    &mt LSHIFT F  &mt LEFT_GUI G  &mt LEFT_GUI H  &mt LSHIFT J  &mt RALT K    &mt RIGHT_CONTROL L  &mt RIGHT_CONTROL SEMICOLON
&mt LEFT_SHIFT Z    &kp X        &kp C             &kp V         &kp B           &kp N           &kp M         &kp COMMA     &kp PERIOD           &mt RSHIFT SLASH
                                 &mt LEFT_ALT TAB  &lt 2 ESCAPE  &kp ENTER       &kp BSPC        &kp SPACE     &kp LEFT_GUI
            >;
